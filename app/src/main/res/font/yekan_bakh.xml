<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <!-- light -->
    <font
        android:font="@font/yekan_bakh_light"
        android:fontStyle="normal"
        android:fontWeight="300"
        app:font="@font/yekan_bakh_light"
        app:fontStyle="normal"
        app:fontWeight="300" />

    <!-- regular -->
    <font
        android:font="@font/yekan_bakh_regular"
        android:fontStyle="normal"
        android:fontWeight="400"
        app:font="@font/yekan_bakh_regular"
        app:fontStyle="normal"
        app:fontWeight="400" />


    <!-- medium -->
    <font
        android:font="@font/yekan_bakh_medium"
        android:fontStyle="normal"
        android:fontWeight="500"
        app:font="@font/yekan_bakh_medium"
        app:fontStyle="normal"
        app:fontWeight="500" />

    <!-- bold -->
    <font
        android:font="@font/yekan_bakh_bold"
        android:fontStyle="normal"
        android:fontWeight="700"
        app:font="@font/yekan_bakh_bold"
        app:fontStyle="normal"
        app:fontWeight="700" />
</font-family>